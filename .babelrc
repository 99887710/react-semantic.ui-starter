{
  "plugins": [
    "lodash",
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ],
    "react-hot-loader/babel",
    "syntax-dynamic-import",
    [
      "babel-plugin-webpack-alias",
      {
        "config": "./webpack_config/webpack.base.js",
        "findConfig": true
      }
    ],
    "dynamic-import-node"
  ],
  "presets": ["es2015", "react", "stage-0"],
  "env": {
    "production": {
      "presets": ["react-optimize"]
    }
  }
}
